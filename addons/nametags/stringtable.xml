    <Package name="NameTags">
        <Key ID="STR_A4ES_NameTags_Settings_Addon">
            <English>A4ES Name Tags</English>
            <Polish>A4ES Name Tags</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_show2DNameTag">
            <English>Show 2D tag</English>
            <Polish>Wyświetl tag 2D</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_show2DNameTag_Desc">
            <English>Show 2D tag on cursor</English>
            <Polish>Wyświetl tag 2D pod kursorem</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_show3DNameTag">
            <English>Show 3D tag</English>
            <Polish>Wyświetl tag 3D</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_show3DNameTag_Desc">
            <English>Show 3D tag above unit head</English>
            <Polish>Wyświetl tag 3D nad głowami jednostek</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_show3DNameTag_Freelook">
            <English>Only during freelook</English>
            <Polish>Tylko podczas rozglądania (domyślnie lewy alt)</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_show3DNameTag_OnCursor">
            <English>Only on cursor</English>
            <Polish>Tylko po najechaniu kursorem</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_show3DNameTag_Both">
            <English>In both cases</English>
            <Polish>W obu przypadkach</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_showSpeaking">
            <English>Show speaking</English>
            <Polish>Wyświetl ikonkę mówienia</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_showSpeaking_Desc">
            <English>Show animation above unit head when speaking (ACRE) on 3D tag</English>
            <Polish>Wyświetl animację nad głową jednostki podczas mówienia (ACRE) na tagu 3D</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_showLeaderIcon">
            <English>Show group leader icon</English>
            <Polish>Wyświetl ikonkę dowódcy drużyny</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_showLeaderIcon_Desc">
            <English>Show group leader icon above unit head on 3D tag</English>
            <Polish>Wyświetl ikonkę dowódcy drużyny na tagu 3D</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_2DNameTagShowTraits">
            <English>Show unit traits on 2D tag</English>
            <Polish>Wyświetl uprawnienia jednostki na tagu 2D</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_2DNameTagPosition">
            <English>2D tag position on screen</English>
            <Polish>Miejsce wyświetlania taga 2D na ekranie</Polish>
        </Key>
        <Key ID="STR_A4ES_NameTags_Settings_2DNameTagPosition_Center">
