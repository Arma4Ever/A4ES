    <Key ID="STR_A4ES_NameTags_Settings_Addon">
      <English>A4ES Name Tags</English>
    </Key>
    <Key ID="STR_A4ES_NameTags_Settings_show2DNameTag">
      <English>Show 2D tag</English>
      <Polish>Wyświetl tag 2D</Polish>
    </Key>
    <Key ID="STR_A4ES_NameTags_Settings_show2DNameTag_Desc">
      <English>Show 2D tag on cursor</English>
      <Polish>Wyświetl tag 2D pod kursorem</Polish>
    </Key>
    <Key ID="STR_A4ES_NameTags_Settings_show3DNameTag">
      <English>Show 3D tag</English>
      <Polish>Wyświetl tag 3D</Polish>
    </Key>
    <Key ID="STR_A4ES_NameTags_Settings_show3DNameTag_Desc">
      <English>Show 3D tag above unit head</English>
      <Polish>Wyświetl tag 3D nad głowami jednostek</Polish>
    </Key>
    <Key ID="STR_A4ES_NameTags_Settings_show3DNameTag_Freelook">
      <English>Only during freelook</English>
      <Polish>Tylko podczas rozglądania (domyślnie lewy alt)</Polish>
    </Key>
    <Key ID="STR_A4ES_NameTags_Settings_show3DNameTag_OnCursor">
      <English>Only on cursor</English>
      <Polish>Tylko po najechaniu kursorem</Polish>
    </Key>
